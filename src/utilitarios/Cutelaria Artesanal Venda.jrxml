	<queryString>
		<![CDATA[SELECT
     *,
     funcionario.`id_funcionario` AS funcionario_id_funcionario,
     funcionario.`nome_func` AS funcionario_nome_func,
     funcionario.`cpf_func` AS funcionario_cpf_func,
     funcionario.`rg_func` AS funcionario_rg_func,
     funcionario.`estadoCivil_func` AS funcionario_estadoCivil_func,
     funcionario.`sexo_func` AS funcionario_sexo_func,
     funcionario.`dataNasc_func` AS funcionario_dataNasc_func,
     funcionario.`salario_func` AS funcionario_salario_func,
     funcionario.`telefone_func` AS funcionario_telefone_func,
     funcionario.`funcao_func` AS funcionario_funcao_func,
     funcionario.`endereco_func` AS funcionario_endereco_func,
     funcionario.`senha_func` AS funcionario_senha_func,
     cliente.`id_cliente` AS cliente_id_cliente,
     cliente.`nome_cli` AS cliente_nome_cli,
     cliente.`cpf_cli` AS cliente_cpf_cli,
     cliente.`rg_cli` AS cliente_rg_cli,
     cliente.`estadoCivil_cli` AS cliente_estadoCivil_cli,
     cliente.`sexo_cli` AS cliente_sexo_cli,
     cliente.`dataNasc_cli` AS cliente_dataNasc_cli,
     cliente.`telefone_cli` AS cliente_telefone_cli,
     cliente.`endereco_cli` AS cliente_endereco_cli
FROM
     `funcionario` funcionario INNER JOIN `venda` venda ON funcionario.`id_funcionario` = venda.`id_funcionario`
     INNER JOIN `cliente` cliente ON venda.`id_cliente` = cliente.`id_cliente`]]>
	</queryString>
